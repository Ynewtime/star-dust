{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "start:nomock": "MOCK=none umi dev",
    "build": "umi build",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "postinstall": "umi generate tmp",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@ant-design/pro-layout": "^5.0.12",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.22",
    "ahooks": "^2.6.1",
    "antd": "^4.6.5",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.20",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "recoil": "0.0.13",
    "tailwindcss": "^1.8.10",
    "umi": "^3.2.22",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@umijs/fabric": "^2.2.2",
    "@umijs/plugin-esbuild": "^1.0.1",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "stylelint": "^13.7.1"
  }
}
